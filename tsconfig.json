{
  "compilerOptions": {
      "jsx": "react",
      "target": "es6",
      "sourceMap": true,
      "noImplicitAny": false,
      "experimentalDecorators":true,
      "allowJs": true,
      "paths":{
        "src":["./src"]
      }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
} 